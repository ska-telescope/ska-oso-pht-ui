{
  "author": "Stargazers - OMC ART",
  "description": "SKA OSO PHT Application",
  "name": "ska-oso-pht-ui",
  "private": true,
  "license": "BSD 3-Clause",
  "repository": "git@https://gitlab.com/ska-telescope/ska-oso-pht-ui.git",
  "type": "module",
  "version": "1.0.4",
  "exports": {
    ".": {
      "import": "./esm/main.tsx",
      "require": "./main.tsx"
    }
  },
  "scripts": {
    "build": "vite build",
    "checker": "yarn prettier:fix && yarn lint:fix",
    "code-analysis": "eslint src/**/*.{js,jsx,ts,tsx} --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:ci": "cypress run --reporter junit --reporter-options 'mochaFile=build/reports/cypress-tests-[hash].xml'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:run:component": "cypress run --component --config video=false",
    "dev": "npx react-inject-env set -d public/ && vite",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "local": "vite",
    "merge-component-reports": "jrm build/tests/component-tests.xml \"build/tests/*.xml\"",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "skao:update": "yarn add @ska-telescope/ska-gui-local-storage@latest @ska-telescope/ska-gui-components@latest",
    "start": "npx react-inject-env set -d public/ && NODE_ENV=test vite serve --open --port 6101",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports --reporter=cobertura",
    "test:coverage:summary": "npx nyc report --reporter=text-summary",
    "test:headless": "yarn cypress:run",
    "test:component:headless": "cypress run --component --headless --config video=false",
    "test:cypress:e2e:ci": "cypress run --e2e --headless --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "test:cypress:component:ci": "cypress run --component --headless --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml --config video=false",
    "test:e2e:headless": "cypress run --e2e --headless",
    "test:e2e:coverage": "nyc report --report-dir=build/reports --reporter=cobertura",
    "test:e2e": "cypress run --spec=cypress/e2e/**/*.test.tsx video=false",
    "test:e2e:ui": "cypress open",
    "test:unit": "vitest --reporter=junit --outputFile=build/reports/unit-tests.xml --typecheck --coverage.enabled=true --coverage.reportsDirectory=build/coverage",
    "test:unit:ui": "vitest  --coverage.enabled=true  --reporter=default --ui",
    "typecheck": "tsc --noEmit"
  },
