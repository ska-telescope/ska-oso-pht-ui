{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["react-app", "prettier", "plugin:cypress/recommended"],
  "plugins": ["prettier", "cypress"],
  "rules": {
    "import/order": 1,
    "no-console": 2,
    "linebreak-style": 0,
    "prettier/prettier": ["error"],
    "react-hooks/exhaustive-deps": 0
  }
}
